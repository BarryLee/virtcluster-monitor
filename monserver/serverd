#!/usr/bin/env python
from includes.daemonize import startstop
from utils.load_config import load_global_config

from sys import stderr as sys_stderr

config = load_global_config()
ofile = config['ofile']
efile = config['efile']
pfile = config['pfile']

startstop(stdout=ofile, 
          stderr=efile,
          pidfile=pfile)
from server import main
main()

